<Page
    x:Class="Mid_Project.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Mid_Project"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid BorderBrush="{ThemeResource AppBarBackgroundThemeBrush}">
        <Grid.Background>
            <ImageBrush ImageSource="Assets/background3.jpg" Stretch="UniformToFill" Opacity="1"/>
        </Grid.Background>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="100" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Image x:Name="image" Grid.Row="0" Source="Assets/logo.png" HorizontalAlignment="Center" Height="220" Width="490" />
        <!--<AutoSuggestBox QueryIcon="Find" Grid.Row="1" x:Name="text" QuerySubmitted="query_Click" PlaceholderText="请输入所要查询的号码或姓名" Width="300" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>-->
        <Button x:Name="enter_button" Content="进入通讯录" Foreground="LightYellow" Grid.Row="2" HorizontalAlignment="Center" Click="enterbutton_Click" FontSize="29.333" Height="50" Width="200"/>
    </Grid>
</Page>
